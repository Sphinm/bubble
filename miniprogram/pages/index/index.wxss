/* 气泡 */
page {
  background: #fff;
}
@keyframes circel {
  0% {
    transform: scale(1);
  }
  50% {
    transform: scale(1.091);
  }
  100% {
    transform: scale(1);
  }
}
@keyframes littleBubble {
  0% {
    transform: translateY(0);
  }
  50% {
    transform: translateY(2vw);
  }
  100% {
    transform: translateY(0);
  }
}
@keyframes scaleEase {
  0% {
    transform: scale(0.96);
  }
  50% {
    transform: scale(1);
  }
  100% {
    transform: scale(0.96);
  }
}
canvas {
  width: 100%;
  height: 400rpx;
}
/* 授权弹板 */
.login_btn {
  position: fixed;
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  z-index: 1000;
  opacity: 0;
}
.burnTop {
  top: 72rpx !important;
}
.burnCoin {
  position: absolute;
  left: 24rpx;
  top: 24rpx;
  display: flex;
  align-items: center;
}
.burnCoin .coinImg {
  position: relative;
  left: 0;
  width: 38rpx;
  height: 38rpx;
  z-index: 2;
}
.burnCoin .coinNum {
  height: 36rpx;
  line-height: 36rpx;
  border-radius: 18rpx;
  box-sizing: border-box;
  border: 1rpx solid #ff9e34;
  padding: 0 16rpx 0 36rpx;
  margin-left: -30rpx;
  background-color: #fff;
  color: #f8b12b;
  font-size: 26rpx;
  z-index: 1;
}
.exchangeRoll {
  height: 54rpx;
  background-color: rgba(255, 70, 9, 0.102);
}
.exchangeRoll .exchangeRoll-swiper {
  height: 54rpx;
}
.exchangeRoll .roll-item {
  font-size: 24rpx;
  color: #ff4609;
  text-align: center;
  line-height: 54rpx;
}
.exchangeRoll .roll-item .headImg {
  width: 36rpx;
  height: 36rpx;
  border-radius: 18rpx;
  vertical-align: middle;
  margin-top: -4rpx;
  background: #b476eb;
}
.bubble {
  position: relative;
  height: 550rpx;
  margin: 20rpx 30rpx 50rpx 30rpx;
  box-sizing: border-box;
}
.bubble .tip_hook {
  position: absolute;
  width: 100%;
  height: 100%;
}
.bubble .tip_hook .step_icon {
  width: 100rpx;
  height: 100rpx;
  text-align: center;
  animation: littleBubble 2s infinite ease-in;
}
.bubble .tip_hook .step_icon .today {
  position: relative;
}
.bubble .tip_hook .step_icon .today .run_scope {
  position: absolute;
  top: -100rpx;
  left: 0;
  width: 100rpx;
  height: 100rpx;
}
.bubble .tip_hook .step_icon .today .run_scope .num {
  font-family: PingFangHK-Regular;
  margin: 30rpx 0 10rpx;
  font-size: 24rpx;
}
.bubble .tip_hook .step_icon .today .tip_title {
  margin-top: 104rpx;
  font-size: 20rpx;
  font-family: PingFangHK-Regular;
  font-weight: 400;
  white-space: nowrap;
}
.bubble .tip_hook .step_0 {
  position: absolute;
  top: 60rpx;
  left: 80rpx;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 100% 100%;
}
.bubble .tip_hook .step_0 .num,
.bubble .tip_hook .step_0 .tip_title {
  color: #3f8bf6;
}
.bubble .tip_hook .step_1 {
  position: absolute;
  top: 0rpx;
  right: 110rpx;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 100% 100%;
}
.bubble .tip_hook .step_1 .num,
.bubble .tip_hook .step_1 .tip_title {
  color: #cb6efd;
}
.bubble .tip_hook .step_2 {
  position: absolute;
  top: 250rpx;
  left: 30rpx;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 100% 100%;
}
.bubble .tip_hook .step_2 .num,
.bubble .tip_hook .step_2 .tip_title {
  color: #f8be53;
}
.bubble .tip_hook .step_3 {
  position: absolute;
  top: 120rpx;
  right: 0;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 100% 100%;
}
.bubble .tip_hook .step_3 .num,
.bubble .tip_hook .step_3 .tip_title {
  color: #df8997;
}
.bubble .tip_hook .step_4 {
  position: absolute;
  bottom: 160rpx;
  right: 40rpx;
  background: url("data:image/png;base64,") center no-repeat;
  background-size: 100% 100%;
}
.bubble .tip_hook .step_4 .num,
.bubble .tip_hook .step_4 .tip_title {
  color: #3f8bf6;
}
.bubble .total {
  position: relative;
  width: 377rpx;
  height: 550rpx;
  margin-left: 50%;
  transform: translateX(-50%);
  text-align: center;
}
.bubble .total .stepExchange {
  position: absolute;
  top: 100rpx;
  left: 50%;
  width: 326rpx;
  height: 326rpx;
  transform: translateX(-50%);
}
.bubble .total .stepExchange .circel {
  width: 327rpx;
  height: 327rpx;
  border-radius: 50%;
  font-family: "PingFang-SC-Regular";
  overflow: hidden;
  animation: circel 1.667s linear infinite;
}
.bubble .total .stepExchange .circel .circelimg {
  position: absolute;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  z-index: 0;
  border-radius: 50%;
}
.bubble .total .stepExchange .circel view {
  position: absolute;
  left: 50%;
  z-index: 1;
  transform: translate(-50%);
}
.bubble .total .stepExchange .circel .number {
  height: 70rpx;
  line-height: 70rpx;
  top: 112rpx;
  font-size: 70rpx;
  font-family: "Roboto";
  font-weight: 400;
  color: #ffffff;
}
.bubble .total .stepExchange .circel .today {
  height: 20rpx;
  line-height: 20rpx;
  top: 85rpx;
  font-size: 20rpx;
  font-family: PingFang-SC-Regular;
  color: rgba(255, 255, 255, 0.8);
}
.bubble .total .stepExchange .circel .day1 {
  height: 28rpx;
  line-height: 28rpx;
  top: 198rpx;
  font-size: 28rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #ffffff;
}
.bubble .total .stepExchange .circel .day2 {
  height: 16rpx;
  line-height: 16rpx;
  top: 238rpx;
  font-size: 16rpx;
  font-family: PingFang-SC-Regular;
  font-weight: 400;
  color: #ffffff;
  opacity: 0.6;
}
.bubble .total .on_share {
  position: absolute;
  bottom: 40rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 435rpx;
  height: 80rpx;
  line-height: 80rpx;
  background: linear-gradient(125deg, #636df2 30%, #9732ee);
  box-shadow: 6rpx 10rpx 10rpx #999;
  border-radius: 50rpx;
  text-align: center;
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
  font-weight: 500;
  color: #fff;
}
.bubble .total .on_share button:after {
  border: none;
}
.bubble .total .on_share button {
  position: absolute;
  top: 0;
  left: 0;
  width: inherit;
  height: inherit;
  z-index: 110;
  background: transparent;
}
.bubble .total .on_share .share_icon {
  position: absolute;
  top: 50%;
  right: 50rpx;
  transform: translateY(-50%);
  width: 30rpx;
  height: 30rpx;
}
.global_mask {
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background: rgba(0, 0, 0, 0.5);
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  z-index: 10000;
}
.global_mask .change_box {
  width: 540rpx;
  height: 270rpx;
  background: #fff;
  border-radius: 10rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.global_mask .change_box .change_tip {
  font-size: 30rpx;
  line-height: 40rpx;
  padding: 62rpx 0 48rpx;
  text-align: center;
  color: #404040;
}
.global_mask .change_box .change_btn_box {
  width: 456rpx;
  height: 70rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.global_mask .change_box .change_btn_box .change_btn {
  width: 200rpx;
  height: 70rpx;
  line-height: 70rpx;
  background: linear-gradient(90deg, #765efe, #b66ce0);
  border-radius: 40rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  position: relative;
}
.global_mask .change_box .change_btn_box text {
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 196rpx;
  height: 64rpx;
  line-height: 64rpx;
  text-align: center;
  font-size: 30rpx;
  color: #7861ff;
  background: #fff;
  border-radius: 38rpx;
}
.juhe_data {
  width: 90%;
  height: 200rpx;
  margin: 100rpx auto;
  background: #eee;
}
.juhe_data .menu-item {
  width: 100%;
  height: 260rpx;
  box-shadow: 1px 1px 5px 1px #eee;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
  font-size: 42rpx;
  color: #fff;
