.lists {
    width: 720rpx;
    margin: 0 auto;
    display: flex;
    flex-wrap: wrap;
}

.lists .list {
    flex: 0 0 360rpx;
    margin-bottom: 30rpx;
    position: relative;
}

.lists .list image {
    display: block;
    width: 310rpx;
    height: 310rpx;
    margin: 0 auto;
    margin-bottom: 16rpx;
}

.lists .list .title {
    width: 310rpx;
    font-size: 30rpx;
    margin: 0 auto;
    color: #484848;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
    margin-bottom: 16rpx;
}

.price {
    width: 310rpx;
    display: flex;
    margin: 0 auto;
    height: 34rpx;
    line-height: 34rpx;
    margin-bottom: 16rpx;
}

.price .price_left {
    flex: 1;
    font-size: 24rpx;
    color: #fc4838;
}

.price .price_left text {
    font-size: 34rpx;
}

.price .price_right {
    flex: 0 0 120rpx;
    font-size: 24rpx;
    color: #666;
    text-align: right;
}

.vouchers {
    width: 310rpx;
    height: 45rpx;
    margin: 0 auto;
    position: relative;
}

.vouchers .vouchers_left {
    width: 126rpx;
    height: 45rpx;
    position: absolute;
    top: 0;
    left: 0;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
    color: #fff;
    line-height: 45rpx;
    font-size: 30rpx;
    text-align: center;
}

.vouchers .vouchers_right {
    width: 136rpx;
    height: 40rpx;
    line-height: 40rpx;
    position: absolute;
    top: 3rpx;
    right: 0;
    text-align: center;
    border: 1rpx solid #888;
    color: #e18127;
    font-size: 26rpx;
}

.tab {
    width: 670rpx;
    height: 82rpx;
    line-height: 82rpx;
    margin: 0 auto;
    background: #f5f5f7;
    margin-bottom: 30rpx;
    display: flex;
}

.tab .tab_flex {
    flex: 1;
    text-align: center;
    font-size: 28rpx;
    color: #666;
}

.tab .active text {
    display: inline-block;
    color: #000;
    height: 82rpx;
    line-height: 82rpx;
    border-bottom: 3rpx solid #e18127;
}

.lists_top {
    width: 750rpx;
    height: 288rpx;
    text-align: center;
    box-sizing: border-box;
    line-height: 80rpx;
    font-size: 32rpx;
    color: #fff;
    position: relative;
}

.lists_top image {
    display: block;
    width: 100%;
    height: 100%;
}

.lists_top view {
    position: absolute;
    text-align: center;
    width: 100%;
    top: 160rpx;
    left: 0;
}

.list .box {
    position: absolute;
    width: 142rpx;
    height: 97rpx;
    box-sizing: border-box;
    bottom: 10rpx;
    right: 30rpx;
}

.list .box image {
    display: block;
    width: 100%;
    height: 100%;
}

.adver {
    width: 700rpx;
    margin: 0 auto;
    margin-bottom: 30rpx;
}

.modal {
    position: fixed;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.5);
}

.modal14 {
    box-sizing: border-box;
    padding-top: 230rpx;
}

.modal14 .modal14_title {
    font-size: 34rpx;
    color: #fff;
    text-align: center;
    letter-spacing: 3rpx;
    margin-bottom: 120rpx;
}

.modal14 .modal14_title text {
    color: #ffa820;
}

.modal14 image {
    display: block;
    width: 200rpx;
    height: 198rpx;
    margin: 0 auto;
    margin-bottom: 50rpx;
}

.modal14 .modal14_text {
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    margin-bottom: 50rpx;
}

.modal14 button {
    width: 300rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
    color: #fff;
    background: #ffa820;
}

.modal .close {
    display: block;
    width: 80rpx;
    height: 80rpx;
    margin: 0 auto;
    position: absolute;
    left: 335rpx;
    top: 850rpx;
}

.topback {
    position: fixed;
    bottom: 100rpx;
    right: 30rpx;
    width: 100rpx;
    height: 130rpx;
}

.topback image {
    width: 100rpx;
    height: 100rpx;
    margin: 0 auto;
}

.topback view {
    height: 30rpx;
    line-height: 30rpx;
    font-size: 24rpx;
    color: #00d7a7;
    text-align: center;
}