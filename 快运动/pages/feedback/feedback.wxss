.title {
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    box-sizing: border-box;
    padding: 0 20rpx;
    color: #7a7677;
}

.obj {
    height: 100rpx;
    line-height: 100rpx;
    background: #fff;
    box-sizing: border-box;
    padding: 20rpx;
    display: flex;
}

.obj .obj_left {
    flex: 0 0 60rpx;
    height: 60rpx;
    border-radius: 80rpx;
    overflow: hidden;
}

.obj .obj_left image {
    display: block;
    width: 100%;
    height: 100%;
}

.obj .obj_right {
    flex: 1;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 34rpx;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.main {
    background: #fff;
    box-sizing: border-box;
    padding-left: 20rpx;
}

.main .one {
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    border-bottom: 1rpx solid #eee;
}

.main textarea {
    width: 100%;
    font-size: 30rpx;
    line-height: 40rpx;
    box-sizing: border-box;
    padding: 20rpx;
    padding-left: 0;
    border-bottom: 1rpx solid #eee;
}

.main .two {
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    display: flex;
    box-sizing: border-box;
    padding-right: 20rpx;
}

.main .two .two_left {
    text-align: left;
    flex: 1;
}

.main .two .two_right {
    flex: 1;
    text-align: right;
    font-size: 26rpx;
    color: #7d7d7d;
}

.main .img {
    box-sizing: border-box;
    padding-bottom: 20rpx;
}

.main .img .imgs {
    display: inline-block;
    width: 120rpx;
    height: 120rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
}

.main .img .imgs image {
    display: block;
    width: 100%;
    height: 100%;
}

button {
    width: 95%;
    height: 80rpx;
    line-height: 80rpx;
    margin-top: 50rpx;
    background: #46ac2c;
    color: #fff;
    font-size: 35rpx;
    letter-spacing: 10rpx;
}