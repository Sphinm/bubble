page {
    background: url("https://diandian-1258683431.cos.ap-guangzhou.myqcloud.com/campaign3/bg.png")no-repeat;
    background-size: 100% 100%;
}

.guize {
    width: 150rpx;
    height: 50rpx;
    background: rgba(255,255,255,0.4);
    border-radius: 50rpx;
    line-height: 50rpx;
    text-align: center;
    font-size: 24rpx;
    color: #fff;
    position: fixed;
    top: 20rpx;
    left: 20rpx;
}

.mainimg {
    display: block;
    width: 439rpx;
    height: 384rpx;
    margin: 0 auto;
    margin-top: 30rpx;
}

.titles {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 30rpx;
    color: #fff;
    text-align: center;
    margin-top: 30rpx;
}

.time {
    display: block;
    width: 686rpx;
    height: 389rpx;
    margin: 0 auto;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
    margin-top: 30rpx;
    box-sizing: border-box;
    padding-top: 110rpx;
}

.time .time_num {
    font-size: 90rpx;
    text-align: center;
    letter-spacing: 3rpx;
    height: 100rpx;
    line-height: 100rpx;
}

.time button {
    width: 500rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    font-size: 36rpx;
    background: #f7cb41;
    color: #fff;
    -webkit-border-after: 100rpx;
    border-block-end: 100rpx;
    margin-top: 60rpx;
}

.title {
    display: flex;
    margin-top: 100rpx;
    box-sizing: border-box;
    padding: 0 15rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
}

.title .title_img {
    flex: 0 0 24rpx;
}

.title .title_img image {
    display: block;
    width: 24rpx;
    height: 28rpx;
    vertical-align: top;
    margin-top: 24rpx;
}

.title .title_view {
    flex: 1;
}

.title .title_view .title_view_view {
    display: block;
    width: 70rpx;
    height: 70rpx;
    overflow: hidden;
    background: rgba(0,0,0,0.4);
    border-radius: 70rpx;
    line-height: 70rpx;
    margin: 0 auto;
    box-sizing: border-box;
}

.title .title_view .title_view_view .title_view_icon {
    display: block;
    width: 50rpx;
    height: 50rpx;
    line-height: 50rpx;
    background: #fda23e;
    border-radius: 60rpx;
    margin: 0 auto;
    margin-top: 10rpx;
}

.title .title_view .title_view_img {
    height: 50rpx;
    line-height: 50rpx;
    margin-top: 10rpx;
    letter-spacing: 2rpx;
}

.title .title_view .title_view_img image {
    display: inline-block;
    width: 36rpx;
    height: 36rpx;
    vertical-align: top;
    margin-top: 7rpx;
}

.topic {
    width: 686rpx;
    height: 635rpx;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-top: 50rpx;
    box-sizing: border-box;
    padding-top: 30rpx;
}

.topic_title {
    font-size: 46rpx;
    padding-left: 30rpx;
    margin-bottom: 20rpx;
}

.topic_text {
    font-size: 32rpx;
    padding-left: 30rpx;
    margin-bottom: 100rpx;
}

.topic_topic {
    font-size: 34rpx;
    letter-spacing: 2rpx;
    line-height: 50rpx;
    text-align: justify;
    margin-bottom: 40rpx;
    box-sizing: border-box;
    padding: 0 50rpx;
}

.topic_btn {
    width: 500rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    margin: 0 auto;
    background: #eee;
    text-align: center;
    margin-bottom: 30rpx;
    font-size: 34rpx;
}

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,0.3);
    position: fixed;
    top: 0;
    left: 0;
}

.modal1 {
    width: 600rpx;
    height: 650rpx;
    margin: 0 auto;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin-top: 150rpx;
    box-sizing: border-box;
    padding-top: 60rpx;
}

.modal1 .modal1_title {
    display: block;
    width: 371rpx;
    height: 48rpx;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-bottom: 30rpx;
}

.modal1 .modal1_text {
    text-align: center;
    font-size: 36rpx;
    color: #fff;
    margin-bottom: 30rpx;
}

.modal1 .modal1_view {
    width: 100%;
    height: 120rpx;
    line-height: 120rpx;
    text-align: center;
    font-size: 80rpx;
    color: #fff;
    letter-spacing: 20rpx;
    font-weight: bold;
    margin-bottom: 30rpx;
}

.modal1 .modal1_view image {
    display: inline-block;
    width: 120rpx;
    height: 120rpx;
    vertical-align: top;
    margin-right: 20rpx;
}

.modal1 .modal1_jiangli {
    display: block;
    width: 300rpx;
    height: 40rpx;
    line-height: 40rpx;
    border-radius: 40rpx;
    margin: 0 auto;
    font-size: 24rpx;
    text-align: center;
    background: #ba46d3;
    color: #e3a2f2;
    letter-spacing: 2rpx;
    margin-bottom: 30rpx;
}

.modal1 .modal1_jiangli text {
    color: #fae251;
}

.modal1 .btn {
    width: 500rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    background: #cd50e8;
    color: #ea8bff;
    margin-bottom: 30rpx;
    font-size: 30rpx;
}

.modal1 .btns {
    width: 500rpx;
    height: 80rpx;
    line-height: 80rpx;
    border-radius: 80rpx;
    background: #f7cb41;
    color: #fff;
    margin-bottom: 30rpx;
    font-size: 30rpx;
}

.modal2 {
    width: 600rpx;
    height: 364rpx;
    margin: 0 auto;
    background: url("data:image/png;base64,")no-repeat;
    margin-top: 360rpx;
    box-sizing: border-box;
    padding-top: 100rpx;
}

.modal2 .modal2_title {
    width: 368rpx;
    height: 48rpx;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-bottom: 60rpx;
}

.modal2 .modal2_text {
    text-align: center;
    font-size: 34rpx;
    color: #fff;
}

.modal3 {
    width: 600rpx;
    height: 450rpx;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-top: 340rpx;
    box-sizing: border-box;
    padding-top: 60rpx;
}

.modal3 .modal3_title {
    display: block;
    width: 219rpx;
    height: 48rpx;
    margin: 0 auto;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin-bottom: 40rpx;
}

.modal3 .modal3_text {
    height: 70rpx;
    line-height: 70rpx;
    font-size: 34rpx;
    color: #fff;
    text-align: center;
    margin-bottom: 10rpx;
}

.modal3 .modal3_jiangji {
    font-size: 34rpx;
    color: #fff;
    text-align: center;
    letter-spacing: 4rpx;
    height: 80rpx;
    line-height: 80rpx;
    margin-bottom: 20rpx;
}

.modal3 .modal3_jiangji image {
    display: inline-block;
    width: 50rpx;
    height: 50rpx;
    vertical-align: top;
    margin-top: 15rpx;
    margin-right: 10rpx;
}

.modal3 .btn {
    width: 500rpx;
    height: 80rpx;
    line-height: 80rpx;
    background: #f7cb41;
    color: #fff;
    border-radius: 80rpx;
    font-size: 38rpx;
}

.modal4 {
    display: block;
    width: 600rpx;
    height: 596rpx;
    background: url("data:image/png;base64,")no-repeat;
    background-size: 100% 100%;
    margin: 0 auto;
    margin-top: 150rpx;
}

.modal .close {
    position: absolute;
    width: 750rpx;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    font-size: 30rpx;
    color: #fff;
    left: 0;
    top: 800rpx;
}

.adver {
    width: 100%;
    position: fixed;
    bottom: 0;
    left: 0;
}

.adver ad {
    width: 100%;
}