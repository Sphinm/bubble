page {
    background: #42e1bd;
}

.logo {
    display: block;
    width: 326rpx;
    height: 57rpx;
    margin: 30rpx auto;
}

.paihang {
    width: 690rpx;
    height: 884rpx;
    margin: 0 auto;
    background: url("data:image/png;base64,") no-repeat;
    background-size: 100% 100%;
    box-sizing: border-box;
    padding-top: 50rpx;
    position: relative;
}

.paihang .title {
    width: 470rpx;
    height: 50rpx;
    line-height: 50rpx;
    box-sizing: border-box;
    font-size: 30rpx;
    color: #fff;
    background: #fd3d3c;
    margin: 0 auto;
    text-align: center;
    border-radius: 15rpx;
    border: 2rpx solid #ffe789;
    letter-spacing: 2rpx;
    margin-bottom: 60rpx;
}

.paihang .title text {
    color: #fad23f;
    margin-right: 10rpx;
}

.paihang .jiang {
    text-align: center;
    margin-bottom: 85rpx;
}

.paihang .jiang view {
    display: inline-block;
    width: 60rpx;
    height: 80rpx;
    background: #fd3d3c;
    vertical-align: top;
    font-size: 60rpx;
    color: #fff;
    border-radius: 10rpx;
    margin: 0 10rpx;
}

.paihang .jiang .num {
    line-height: 80rpx;
}

.paihang .jiang .zong {
    font-size: 35rpx;
    color: #fad23f;
    box-sizing: border-box;
    padding-top: 2rpx;
}

.paihang .jiang .zong text {
    font-size: 20rpx;
}

.paihang .time {
    text-align: center;
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 30rpx;
    color: #fd3d3c;
}

.paihang scroll-view {
    width: 100%;
    height: 485rpx;
    background: #42e1bd;
    border-bottom-left-radius: 15rpx;
    border-bottom-right-radius: 15rpx;
    overflow: hidden;
}

.paihang .list {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 32rpx;
    display: flex;
    color: #79567e;
    background: #fff;
}

.paihang .dan {
    background: rgba(255,255,255,0.6);
}

.paihang .list .one {
    flex: 0 0 150rpx;
    box-sizing: border-box;
    padding-left: 30rpx;
}

.paihang .list .fou {
    flex: 0 0 140rpx;
    text-align: center;
}

.paihang .list .two {
    flex: 1;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 1;
    overflow: hidden;
}

.paihang .list .thr {
    flex: 0 0 180rpx;
}

.paihang .ding {
    position: absolute;
    bottom: 0;
    left: 0;
    border-bottom-left-radius: 15rpx;
    border-bottom-right-radius: 15rpx;
}

.btn {
    width: 500rpx;
    height: 80rpx;
    border-radius: 80rpx;
    background: #ffc345;
    color: #fd3d3c;
    line-height: 80rpx;
    font-size: 40rpx;
    margin: 40rpx auto;
}

.modal {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: rgba(0,0,0,0.6);
}

.modal .modal_view {
    width: 600rpx;
    height: 400rpx;
    border-radius: 15rpx;
    margin: 0 auto;
    margin-top: 150rpx;
    background: #fff;
    box-sizing: border-box;
    padding-top: 50rpx;
}

.modal image {
    display: block;
    width: 80rpx;
    height: 80rpx;
    margin: 0 auto;
    margin-top: 100rpx;
}

.modal .modal_view .title {
    text-align: center;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 45rpx;
    margin-bottom: 35rpx;
}

.modal .modal_view .text {
    text-align: center;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 32rpx;
}

.modal .adver {
    width: 100%;
    margin: 0 auto;
    overflow: hidden;
    margin-top: 50rpx;
}