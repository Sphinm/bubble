.change-success {
    min-height: 100vh;
    width: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #f8f8f8;
}

.change-success .success-tip {
    display: flex;
    justify-content: center;
    align-items: center;
    height: 72rpx;
    padding: 60rpx 0 10rpx;
}

.change-success .success-tip image {
    width: 72rpx;
    height: 72rpx;
    display: block;
}

.change-success .success-tip text {
    display: block;
    line-height: 72rpx;
    padding-left: 10rpx;
    font-size: 52rpx;
    color: #7861FF;
}

.change-success .guide-tip {
    line-height: 56rpx;
    font-size: 24rpx;
    color: #868686;
}

.change-success .money-tip {
    line-height: 36rpx;
    padding: 0 20rpx;
    background: rgba(254,107,79,0.1);
    border-radius: 10rpx;
    font-size: 22rpx;
    color: #FE6B4F;
    height: 36rpx;
    margin-bottom: 20rpx;
}

.change-success .change-info {
    width: 610rpx;
    background: #fff;
    border-radius: 20rpx;
}

.change-success .change-info .info-top {
    padding-top: 30rpx;
    display: flex;
    height: 80rpx;
}

.change-success .change-info .info-top .left {
    padding: 0 26rpx 0 30rpx;
}

.change-success .change-info .info-top .left image {
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.change-success .change-info .info-top .right {
    flex: 1;
}

.change-success .change-info .info-top .right .name {
    font-size: 30rpx;
    line-height: 56rpx;
    color: #404040;
}

.change-success .change-info .info-top .right .time {
    font-size: 24rpx;
    line-height: 1;
    color: #868686;
}

.change-success .change-info .info-content {
    padding: 46rpx 135rpx 54rpx;
    display: flex;
    flex-direction: column;
    align-items: flex-start;
}

.change-success .change-info .info-content .change-num {
    height: 40rpx;
    background: rgba(120,97,255,0.1);
    position: relative;
    border-radius: 20rpx;
    display: flex;
    align-items: center;
    padding: 0 36rpx 0 20rpx;
}

.change-success .change-info .info-content .change-num image {
    width: 26rpx;
    height: 24rpx;
    display: block;
}

.change-success .change-info .info-content .change-num .txt {
    padding-left: 10rpx;
    line-height: 40rpx;
    font-size: 24rpx;
    color: #7861FF;
}

.change-success .change-info .info-content .change-num .icon-triangle {
    width: 0;
    height: 0;
    border-right: 9rpx solid transparent;
    border-left: 9rpx solid transparent;
    border-top: 12rpx solid rgba(120,97,255,0.1);
    position: absolute;
    left: 30rpx;
    top: 40rpx;
}

.change-success .change-info .info-content .goods-box {
    padding-top: 20rpx;
    width: 340rpx;
    height: 340rpx;
    position: relative;
}

.change-success .change-info .info-content .goods-box .goods-img {
    width: 340rpx;
    height: 340rpx;
    display: block;
}

.change-success .change-info .info-content .goods-box .good-mask {
    position: absolute;
    left: 0;
    top: 20rpx;
    width: 340rpx;
    height: 340rpx;
    background: rgba(0,0,0,0.4);
