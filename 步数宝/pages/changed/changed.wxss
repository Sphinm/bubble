    left: 48rpx;
    top: 63rpx;
    z-index: 3;
}

.change-success .change-info .info-content .goods-box .frozen {
    position: absolute;
    top: 122rpx;
    left: 0;
    z-index: 4;
    font-size: 32rpx;
    line-height: 40rpx;
    color: #fff;
    text-align: center;
    width: 340rpx;
}

.change-success .change-info .info-content .goods-box .icon-shape {
    width: 384rpx;
    height: 120rpx;
    position: absolute;
    left: -21rpx;
    bottom: 0;
    z-index: 4;
}

.change-success .change-info .info-content .goods-box .exchang {
    line-height: 54rpx;
    padding-left: 10rpx;
    position: absolute;
    left: -22rpx;
