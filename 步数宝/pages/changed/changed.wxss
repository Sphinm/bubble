    right: -22rpx;
    bottom: 60rpx;
    z-index: 5;
}

.change-success .change-info .info-content .goods-box .frozenTime {
    width: 200rpx;
    color: #ffe400;
    font-size: 48rpx;
    line-height: 68rpx;
    text-align: center;
    position: absolute;
    right: -22rpx;
    bottom: 0;
    z-index: 5;
}

