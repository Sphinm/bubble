.returnBtn {
    width: 190rpx;
    height: 60rpx;
    background: linear-gradient(90deg,rgba(120,97,255,1),rgba(182,108,224,1));
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 30rpx;
    border-bottom-right-radius: 30rpx;
    position: fixed;
    color: white;
    bottom: 350rpx;
    left: 0rpx;
    font-size: 30rpx;
    text-align: center;
    z-index: 9999999;
    display: flex;
    flex-direction: row;
    align-items: center;
}

.downLoadApp {
    width: 92rpx;
    height: 92rpx;
    border-radius: 50%;
    position: fixed;
    bottom: 45rpx;
    right: 30rpx;
    z-index: 99;
}

.downLoadApp image {
    width: 92rpx;
    height: 92rpx;
}

.returnImage {
    width: 16rpx;
    height: 27rpx;
    transform: rotate(180deg);
}

.returnBtn .text {
    margin-left: 3rpx;
}

.returnBtn::after {
    border: none;
    border-radius: 0rpx;
}