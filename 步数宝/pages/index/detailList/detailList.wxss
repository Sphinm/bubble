page {
    background: #f8f8f8;
}

.exchange {
    height: auto;
    min-height: 70%;
    background: #f8f8f8;
    padding-bottom: 200rpx;
}

.headTitleBox {
    height: 164rpx;
    width: 750rpx;
    display: flex;
    justify-content: center;
    background: white;
    flex-direction: column;
    border-top: 1rpx solid rgb(221,219,219);
    top: 0;
    z-index: 9;
}

.active {
    margin-top: 30rpx;
    text-align: center;
    font-size: 24rpx;
    color: rgb(134,134,134);
}

.activeNumber {
    text-align: center;
    font-size: 70rpx;
    color: rgb(35,35,35);
}

.smallFont {
    font-size: 28rpx;
    color: rgb(134,134,134);
}

.detailTitle {
    height: 72rpx;
    border-bottom: 1rpx solid rgb(221,219,219);
    background: white;
    color: rgb(134,134,134);
    font-size: 28rpx;
    align-items: center;
    display: flex;
    padding-left: 30rpx;
}

.detailDetaile {
    display: flex;
    text-align: center;
}

.detailDetaileTab {
    height: 120rpx;
    width: 100%;
    background-color: white;
    border-bottom: 1rpx solid rgb(221,219,219);
    display: flex;
}

.fonts {
    position: absolute;
    margin-left: 30rpx;
    margin-top: 30rpx;
    font-weight: 450;
    font-size: 30rpx;
    color: rgb(35,35,35);
}

.fontSmall {
    position: absolute;
    margin-left: 30rpx;
    margin-top: 73rpx;
    font-size: 24rpx;
    color: rgb(134,134,134);
}

.fontsl {
    position: absolute;
    right: 30rpx;
    text-align: center;
    font-size: 30rpx;
    line-height: 120rpx;
    font-weight: 600;
}

.redColor {
    color: rgb(243,93,93);
}

.greenColor {
    color: rgb(72,209,69);
}

.nonepic {
    width: 287rpx;
    height: 224rpx;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 320rpx;
}

.nonepic image {
    width: 100%;
    height: 100%;
}

.nothing {
    font-size: 28rpx;
    color: #ADACB0;
    position: absolute;
    left: 50%;
    transform: translateX(-50%);
    top: 526rpx;
}

.query {
    width: 100%;
    font-size: 22rpx;
    height: 22rpx;
    line-height: 22rpx;
    text-align: center;
    color: rgb(124,124,124);
    padding-top: 50rpx;
    padding-bottom: 110rpx;
    box-sizing: border-box;
    background: #f8f8f8;
}

.swiper {
    background: #f8f8f8;
    width: 690rpx;
    height: 260rpx;
    padding: 0 30rpx;
    position: fixed;
    bottom: 0rpx;
    left: 0;
    display: flex;
}

.slide-image {
    width: 690rpx;
    height: 200rpx;
    padding: 30rpx 0 30rpx 0;
    background: #f8f8f8;
}

.imageadv {
    width: 689rpx;
    height: 200rpx;
}

.adView {
    margin-top: -210rpx;
}

.table-box {
    padding: 0 128rpx;
    height: 100rpx;
    line-height: 100rpx;
    font-size: 30rpx;
    font-weight: 400;
    color: rgba(134,134,134,1);
    overflow: hidden;
}

.table-list-left {
    float: left;
    height: 100%;
    position: relative;
}

.table-list-right {
    float: right;
    height: 100%;
    position: relative;
}

.table-box .active-box {
    font-size: 30rpx;
    font-weight: bold;
    color: rgba(64,64,64,1);
}

.active-box .table-bottom-border {
    width: 114rpx;
    height: 4rpx;
    position: absolute;
    left: 2rpx;
    bottom: 0;
    background: rgba(254,107,79,1);
}

.btn-active {
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: rgba(248,248,248,1);
    font-size: 28rpx;
    font-weight: 400;
    color: rgba(64,64,64,1);
}