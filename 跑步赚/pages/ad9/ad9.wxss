    background-color: #ff5a5a;
    background-image: url(http://litemob-adn.oss-cn-beijing.aliyuncs.com/Mbbc7c6da138195f2ad17388e95ff5d7dgoldicon.png);
    background-repeat: no-repeat;
    background-size: 24rpx 24rpx;
    background-position: 12rpx center;
    border-radius: 24rpx;
}

