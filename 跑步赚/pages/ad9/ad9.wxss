}

.mask_layer {
    width: 100%;
    height: 100%;
    position: fixed;
    z-index: 1000;
    left: 0;
    top: 0;
    background: #000;
    opacity: 0.3;
    overflow: hidden;
}

.modal_box {
    background-color: #fff;
    width: 630rpx;
    height: 426rpx;
    overflow: hidden;
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    margin: auto;
    z-index: 1001;
    text-align: center;
    border-radius: 24rpx;
}

