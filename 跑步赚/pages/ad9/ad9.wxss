.try {
    display: flex;
    justify-content: center;
}

.try-image {
    display: flex;
    align-items: center;
}

.try image {
    width: 36rpx;
    height: 36rpx;
    border-radius: 18rpx;
    margin-right: 12rpx;
}

.try-num {
    color: #373854;
    opacity: 0.5;
    font-size: 24rpx;
    line-height: 36rpx;
}

.cur-ad button {
    width: 480rpx;
    height: 78rpx;
    background-image: linear-gradient(0deg,#ff3a33 0%,#ff9279 100%);
    border-radius: 39rpx;
    color: #fff;
    line-height: 78rpx;
    margin-top: 60rpx;
}

.other-ad {
    background: #fff;
    margin-top: 24rpx;
    padding: 30rpx;
}

.other-ad .header {
    text-align: center;
    color: #373854;
    font-size: 36rpx;
    font-weight: bold;
}

.other-content .item {
    border-radius: 24rpx;
    border: solid 1rpx #f0f0f0;
    display: flex;
    align-items: center;
    padding: 30rpx;
    margin-top: 30rpx;
}

.other-content .item image {
    width: 96rpx;
    height: 96rpx;
}

.other-content .item .otherad-detail {
    flex: 1;
    margin-left: 12rpx;
}

.other-content .item .otherad-detail .otherad-name {
    color: #373854;
    font-size: 30rpx;
    font-weight: bold;
}

.other-content .item .otherad-detail .otherad-des {
    color: #373854;
    opacity: 0.5;
    font-size: 24rpx;
}

.otherad-right {
    text-align: center;
}

.otherad-right .jinbi-num {
    width: 84rpx;
    height: 84rpx;
    line-height: 84rpx;
    color: #be6301;
    font-size: 24rpx;
    background: url("http://litemob-adn.oss-cn-beijing.aliyuncs.com\/M91b57ab39ea1011cdb43fb3ea64f2806%E9%87%91%E5%B8%81%E8%83%8C%E6%99%AF.png") no-repeat;
