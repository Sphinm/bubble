.main-color {
    color: #ef2d2d;
}

.ads {
    background: #f0f0f0;
}

.cur-ad {
    background-color: #fff;
    background-repeat: no-repeat;
    background-size: 750rpx 250rpx;
    height: 720rpx;
    position: relative;
    text-align: center;
}

.icon {
    position: relative;
    z-index: 10;
}

.cur-ad .icon image {
    width: 180rpx;
    height: 180rpx;
    border-radius: 90rpx;
}

.tuoyuan {
    position: absolute;
    width: 100%;
    height: 150rpx;
    top: 215rpx;
    overflow: hidden;
}

.cur-content {
    position: absolute;
    width: 100%;
    top: 124rpx;
}

.tuoyuan:after {
    width: 120%;
    height: 150rpx;
    position: absolute;
    left: -10%;
    top: 0;
    content: '';
    border-radius: 50% 50% 0 0;
    background: #fff;
}

.ad-name {
    font-size: 36rpx;
