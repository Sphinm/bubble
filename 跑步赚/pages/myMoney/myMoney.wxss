    25% {
        transform: rotate(10deg) scale(1.3,1.3);
        opacity: 0.8;
    }

    100% {
        transform: rotate(0deg) scale(0.8,0.8);
        opacity: 1;
    }
}

@keyframes shake {
    0% {
        transform: rotate(0deg) scale(0.8,0.8);
        opacity: 1;
    }

    25% {
        transform: rotate(10deg) scale(1.3,1.3);
