.health {
    margin: 24rpx 30rpx;
    border-radius: 12rpx;
    overflow: hidden;
    box-shadow: 0px 0px 12px 0px rgba(219,219,219,0.75);
    font-size: 0;
}

.health image {
    width: 100%;
    height: 966rpx;
}

.health button {
    width: 690rpx;
    height: 96rpx;
    position: fixed;
    left: 30rpx;
    bottom: 24rpx;
    border-radius: 12rpx;
    background-image: linear-gradient(90deg,#ff4b44 0%,#ff6654 76%,#ff8164 100%);
    color: #fff;
}