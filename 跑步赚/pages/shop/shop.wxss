.order_modal {
    width: 690rpx;
    background: rgba(255,255,255,1);
    box-shadow: 0px 0px 30rpx 0px rgba(219,219,219,0.5);
    border-radius: 30rpx;
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    margin: 0 auto;
    margin-bottom: 30rpx;
}

.row {
    display: flex;
    padding-left: 12rpx;
}

.row image {
    width: 144rpx;
    height: 144rpx;
    margin-left: 12rpx;
}

.order_info {
    margin-left: 12rpx;
    height: 144rpx;
    padding-top: 12rpx;
    padding-bottom: 12rpx;
    box-sizing: border-box;
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.order_info .one {
    font-size: 36rpx;
    color: #222;
    font-weight: bold;
}

.order_price {
    display: flex;
}

.order_price .one {
    font-size: 36rpx;
    color: #ef2d2d;
    font-weight: bold;
}

.order_price .two {
    font-size: 30rpx;
    color: #666;
    text-decoration: line-through;
    height: 100%;
    display: flex;
    flex-direction: column-reverse;
    margin-left: 12rpx;
}

.address {
    width: 100%;
    padding-left: 12rpx;
    padding-top: 24rpx;
    padding-bottom: 24rpx;
    box-sizing: border-box;
    padding-right: 12rpx;
}

.address .one {
    font-size: 24rpx;
    color: #666;
    font-weight: bold;
}

.address .two {
    font-size: 26rpx;
    color: #222222;
    font-weight: bold;
    margin-top: 12rpx;
}

.line {
    width: 100%;
    height: 2rpx;
    background: #F8F8F8;
}

.order_time {
    margin-top: 12rpx;
    height: 100%;
    display: flex;
    padding-left: 12rpx;
    padding-right: 30rpx;
    box-sizing: border-box;
    justify-content: space-between;
}

.order_time .left .one text {
    font-size: 24rpx;
    color: #222222;
    font-weight: bold;
}

.order_time .left .one {
    font-size: 24rpx;
    color: #222222;
}

.order_time .left .two text {
    font-size: 24rpx;
    color: #222222;
    font-weight: bold;
}

.order_time .left .two {
    margin-top: 18rpx;
    font-size: 24rpx;
    color: #222222;
}

.order_time .right {
    height: 100%;
    font-size: 30rpx;
    color: #FF9900;
    font-weight: bold;
    margin-top: 18rpx;
}

.order_time .rights {
    height: 100%;
    font-size: 30rpx;
    color: #14DAB8;
    font-weight: bold;
    margin-top: 18rpx;
}

.order-null {
    width: 328rpx;
    height: 294rpx;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%);
}

.order-null image {
    width: 100%;
    height: 100%;
}

.over_time {
    position: fixed;
    bottom: 0rpx;
    width: 100%;
    padding-top: 30rpx;
    padding-bottom: 30rpx;
    box-sizing: border-box;
    text-align: center;
    font-size: 30rpx;
    color: #999;
